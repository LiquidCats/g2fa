{
    "name": "liquid-cats/g2fa",
    "description": "Google 2FA",
    "type": "library",
    "version": "v0.0.1",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "LiquidCats\\G2FA\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LiquidCats\\G2FA\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Ilya Shabanov",
            "email": "ishabanov@liquid-cats.com"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.2",
        "paragonie/constant_time_encoding": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "friendsofphp/php-cs-fixer": "~v3.41"
    },
    "scripts": {
        "test": "@php vendor/bin/phpunit ./tests",
        "fix": "@php vendor/bin/php-cs-fixer fix"
    }
}
